            <script>
                tinymce.init({
                    selector: '#editableContent',
                    plugins: 'link lists preview',
                    toolbar: 'undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | outdent indent | link',
                    menubar: 'file edit view insert format tools table help',
                    setup: function (editor) {
                        editor.on('change', function () {
                            editor.save();
                        });
                    }
                });

                function saveContent() {
                    const content = tinymce.get('editableContent').getContent();
                    fetch('/save-regulations', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({ content: content })
                    })
                        .then(response => {
                            if (response.ok) {
                                alert('Контент сохранен!');
                                window.location.reload(); // Добавьте это для перезагрузки страницы и отображения нового контента
                            } else {
                                throw new Error('Проблема при сохранении контента');
                            }
                        })
                        .catch(error => {
                            console.error('Ошибка при сохранении:', error);
                            alert('Ошибка при сохранении: ' + error.message);
                        });
                }
            </script>

